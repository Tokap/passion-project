

<form action="/users" method="POST" class="form-container">
	<h2> Sign up! </h2>
  <div class="form-title">Username</div>
  <input class="form-field" type="text" name="user[username]" value="<%= @user.username %>">
  <br>
  <%= erb :"login/_form" %>
 <div class="submit-container"> 
  <input class="submit-button" type="submit" value="Create Account!">
 </div>
</form>